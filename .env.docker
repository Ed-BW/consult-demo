# .env.docker
# Environment variables for the containerized demo environment

# Database
DATABASE_URL=postgres://postgres:postgres@postgres:5432/consultations_dev

# Redis
REDIS_URL=redis://redis:6379/0

# Django
SECRET_KEY='dummy-secret-key-for-demo-only'
DJANGO_SETTINGS_MODULE='consultation_analyser.settings.production'

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# AWS - Not used in demo, but set to avoid errors
AWS_S3_REGION_NAME='eu-west-2'
AWS_STORAGE_BUCKET_NAME='consultation-analyser-uploads'
AWS_DATA_BUCKET='local'
